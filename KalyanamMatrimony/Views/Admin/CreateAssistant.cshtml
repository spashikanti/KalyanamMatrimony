@model KalyanamMatrimony.ViewModels.AssistantViewModel

@{
    ViewData["Title"] = "Create Assistant";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<partial name="~/Views/Partials/_AssistantCreateUpdate.cshtml" model="Model" />

@section Scripts {
    <!-- Page level custom scripts -->
    <script src="~/lib/jquery-validate/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>

    <script src="/js/CustomScript.js"></script>
    <script src="~/js/LoadJsonData.js"></script>

    <script>
        //For toast
        var msg = @Html.Raw(Json.Serialize(ViewBag.Message));
        if (msg != null) {
            displayToast(msg);
        }

        

    </script>
}